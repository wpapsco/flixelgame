		super(X);
		// TODO Auto-generated constructor stub
	}

	public EnemySprite(float X, float Y) {
		super(X, Y);
		// TODO Auto-generated constructor stub
	}

	public EnemySprite(float X, float Y, String SimpleGraphic) {
		super(X, Y, SimpleGraphic);
		// TODO Auto-generated constructor stub
	}

}
